{
  "name": "frame",
  "version": "0.1.0",
  "description": "Tool for publishing web pages to EVM blockspace.",
  "main": "index.js",
  "scripts": {
    "dev": "hardhat compile && nodemon --watch 'src/**/*.ts' --exec ts-node src/index.ts",
    "test-local": "hardhat compile && npx hardhat run src/scripts/local.ts --network localhost",
    "test-goerli": "hardhat compile && npx hardhat run src/scripts/goerli.ts --network goerli",
    "build": "tsc",
    "start": "node dist/index.js"
  },
  "keywords": [
    "typescript",
    "apollo-server",
    "mongoose",
    "jsonwebtoken"
  ],
  "author": "cncrde",
  "license": "ISC",
  "devDependencies": {
    "@openzeppelin/contracts": "^4.8.1",
    "@openzeppelin/hardhat-upgrades": "^1.22.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.7",
    "@types/graphql": "^14.5.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.7.31",
    "@types/node": "^14.0.23",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "graphql": "^15.3.0",
    "nodemon": "^2.0.4",
    "solady": "0.0.73",
    "ts-node": "^8.10.2",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@graphql-tools/schema": "^8.4.0",
    "@nomiclabs/hardhat-ethers": "^2.0.6",
    "@nomiclabs/hardhat-etherscan": "^3.1.5",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@types/graphql": "^14.5.0",
    "@types/ws": "^8.5.3",
    "apollo-server": "^3.9.0",
    "apollo-server-core": "^3.9.0",
    "bcryptjs": "^2.4.3",
    "dot": "^1.1.3",
    "dotenv": "^16.0.1",
    "ethereum-waffle": "^3.4.4",
    "ethereumjs-wallet": "^1.0.2",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.9.0",
    "hardhat": "^2.11.1",
    "hardhat-contract-sizer": "^2.5.1",
    "hardhat-gas-reporter": "^1.0.8",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.24",
    "solidity-coverage": "^0.7.21",
    "subscriptions-transport-ws": "^0.11.0",
    "ws": "^8.8.0"
  }
}
